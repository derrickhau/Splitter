  "source": "pragma solidity ^0.5.0;\n\ncontract Owner {\n    address private owner;\n\n    modifier onlyOwner() {\n        require (msg.sender == owner, \"Restricted access, owner only\");\n        _;\n    }\n    \n    constructor () public {\n        owner = msg.sender;\n    }\n}",
